<% form_with(model: personal_meeting, local:true, :url => {:controller => "schedule", :action=> "save"}) do |form| %>
  <div class="form-row align-items-end">
    <% form.fields_for :schedule_item do |sif| %>
      <div class="form-group col-md-4">
        <%= form.label :start %>
        <%= form.text_field :start, class: 'form-control', required: 'required' %>
      </div>
      <div class="form-group col-md-4">
        <%= form.label :end %>
        <%= form.text_field :end, class: 'form-control', required: 'required' %>
      </div>
    <% end %>
     <div class="form-group col-mb-2">
      <%= form.submit class: 'btn btn-primary' %>
    </div>
<%end%>
