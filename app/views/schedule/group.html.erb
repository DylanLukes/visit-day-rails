<div class="col-md-5 dropdown show">
  <button class="btn btn-secondary dropdown-toggle" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
    <%= @group %>
  </button>

  <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
    <% for group in @groups %>
        <% 
            if group == @group 
               next 
            end
        %>
        <button class="dropdown-item" href="#"><%= group %></button>
    <% end %>
  </div>
</div>
<%= render partial: "calendar/frame", locals: {date: DateTime.now, students: Student.all} %>